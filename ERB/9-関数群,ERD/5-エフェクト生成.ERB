@エフェクト生成
#DIM DYNAMIC LCOUNT
GCREATEFROMFILE 8000, "effect/slash.png"

SPRITEANIMECREATE "斬撃", 240, 240

FOR LCOUNT, 0, 9
	IF LCOUNT == 8
		SPRITEANIMEADDFRAME "斬撃", 8000, LCOUNT*240, 0, 240, 240, 0, 0, POWER(2, 16)
	ELSE
		SPRITEANIMEADDFRAME "斬撃", 8000, LCOUNT*240, 0, 240, 240, 0, 0, 30
	ENDIF
NEXT
