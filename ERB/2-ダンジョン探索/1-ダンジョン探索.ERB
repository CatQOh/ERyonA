@ダンジョン探索
#DIM DYNAMIC LCOUNT
#DIMS DYNAMIC DCOLUMN, 10
#DIM DYNAMIC 削除候補, 11

FOR LCOUNT, 1, 11
	IF 部隊目標:LCOUNT >= 0
		DCOLUMN:LCOUNT = テストダンジョン/ダンジョン{部隊目標:LCOUNT}
		進行度:(部隊目標:LCOUNT) = 1
		ステータス表示中:(部隊目標:LCOUNT) = -1
		アイテム使用中:(部隊目標:LCOUNT) = -1
	ENDIF
NEXT

CALL カラム生成, "ダンジョン探索", DCOLUMN:0, DCOLUMN:1, DCOLUMN:2, DCOLUMN:3, DCOLUMN:4, DCOLUMN:5, DCOLUMN:6, DCOLUMN:7, DCOLUMN:8, DCOLUMN:9

;探索終了処理
;部隊関連
VARSET 削除候補, -1
FOR LCOUNT, 1, 11
	SIF 部隊目標:LCOUNT >= 0
		削除候補:LCOUNT = DT_CELL_GET("ダンジョン", 部隊目標:LCOUNT, "id")
NEXT

VARSET 部隊目標, -1

FOR LCOUNT, 1, 11
	SIF 削除候補:LCOUNT != -1
		DT_ROW_REMOVE "ダンジョン", 削除候補, 10
NEXT

;ダンジョン関連
VARSET 戦闘中, 0
VARSET 進行度, 0
VARSET アイテム表示中, 0
VARSET ステータス表示中, -1
VARSET アイテム使用中, -1
VARSET ダンジョンクリア, 0
オールクリア = 0
VARSET 帰還確認, 0

